                "background-clip",
                "background-color",
                "background-image",
                "background-origin",
                "background-position",
                "-ms-background-position-x",
                "background-position-x",
                "-ms-background-position-y",
                "background-position-y",
                "background-repeat",
                "-moz-background-size",
                "-o-background-size",
                "-webkit-background-size",
                "background-size",
                "border",
                "border-color",
                "border-collapse",
                "-o-border-image",
                "-moz-border-image",
